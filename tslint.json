{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "experimentalDecorators": false,
        "indent": [true, "spaces", 2],
        "semicolon": [true, "never"],
        "quotemark": [true, "single"],
        "no-duplicate-imports": true,
        "interface-name" : [false],
        "no-console": [false, "error"],
        "trailing-comma": [false]
    },
    "rulesDirectory": []
}
